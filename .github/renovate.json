{
	"extends": ["github>donmccurdy/renovate-config"],
	"packageRules": [
		{
			"description": "Node.js v14 EOL",
			"matchPackageNames": ["listr2", "p-limit"],
			"enabled": false
		},
		{
			"description": "Requires changes to greendoc",
			"matchPackageNames": ["ts-morph"],
			"enabled": false
		},
		{
			"description": "Waiting for https://github.com/lovell/sharp/issues?q=is%3Aissue+is%3Aopen+label%3Ainstallation+ to settle",
			"matchPackageNames": ["sharp"],
			"enabled": false
		}
	]
}
