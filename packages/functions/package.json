	"dependencies": {
		"@gltf-transform/core": "^3.0.0",
		"@gltf-transform/extensions": "^3.0.0",
		"ktx-parse": "^0.4.5",
		"ndarray": "^1.0.19",
		"ndarray-lanczos": "^0.2.0",
		"ndarray-pixels": "^1.0.0"
	},
	"files": [
		"dist/",
		"src/",
		"README.md",
		"package.json",
		"package-lock.json"
	],
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "52ef38b8873c12bd1270892e336e9815436bec65"
